{
  "name": "@droppedcode/eslint-plugin-jasmine-memoryleak-linter",
  "version": "1.0.13",
  "description": "An ESLint extension to detect and fix Jasmine / Mocha related memory leaks.",
  "author": "droppedcode",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:test": "tsc -p tsconfig.test.json",
    "build:samples": "npm run build -- --outDir ./samples/node_modules/@droppedcode/eslint-plugin-jasmine-memoryleak-linter",
    "lint": "eslint ./src/** --max-warnings 0",
    "lint:fix": "eslint ./src/** --fix",
    "test": "npm run build:test && jest",
    "pack": "npm run build && npm pack"
  },
  "peerDependencies": {
    "@typescript-eslint/utils": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript-eslint/rule-tester": "^8.13.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-disable-autofix": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.6.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "eslintplugin",
    "eslint-plugin",
    "eslint",
    "jasmine",
    "mocha",
    "memory",
    "memory leak"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/droppedcode/eslint-plugin-jasmine-memoryleak-linter",
  "repository": "git://github.com/droppedcode/eslint-plugin-jasmine-memoryleak-linter"
}
